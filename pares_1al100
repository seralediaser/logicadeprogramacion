<meta charset="UTF-8">
<h1>NUMEROS PARES ENTRE EL 1 & 100</h1>
<script>
    function saltarLinea() {
        document.write("<br>");    
        document.write("<br>");    
        document.write("<br>");    
    }  
    function imprimir(frase) {
        document.write(frase);
        saltarLinea(); 
    } 

    var multiplicador = 1 

    while (multiplicador <= 50 ){

        imprimir(2 * multiplicador);
        multiplicador = multiplicador +1;
    }

    imprimir("fin")

</script>
